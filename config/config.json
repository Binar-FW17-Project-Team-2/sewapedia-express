
{
  "development": {
    "use_env_variable": "DATABASE_URL_DEV",
    "dialect": "postgres"
  },
  "test": {
    "use_env_variable": "DATABASE_URL_TEST",
    "dialect": "postgres",
    "logging": false
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOption": {
      "ssl": true
    }
  }
}

